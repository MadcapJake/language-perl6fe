'repository':
  'variables':
    'patterns': [
        {
          'match': '(?x)
            (\\$|@|%|&)
            (\\.|\\*|:|!|\\^|~|=|\\?|(<(?=.+>)))?
            (
              ([a-zA-Z_\xc0-\xff\\$])
              (
                [a-zA-Z0-9_\xc0-\xff\\$]|
                [\\-\'][a-zA-Z_\xc0-\xff\\$][a-zA-Z0-9_\xc0-\xff\\$]
              )*
            )'
          'captures':
            '1':
              'name': 'support.function.sigil.perl6fe'
            '2':
              'name': 'entity.name.function.twigil.perl6fe'
            '3':
              'name': 'symbol.name.perl6fe'
        }
    ]
  'interpolation':
    'patterns': [
      {
        'match': '(?x)(?<!\\\\)
          (\\$|@|%|&)
          (\\.|\\*|:|!|\\^|~|=|\\?|(<(?=.+>)))?
          ([a-zA-Z_\xc0-\xff\\$])
          (
            [a-zA-Z0-9_\xc0-\xff\\$]|
            [\\-\'][a-zA-Z_\xc0-\xff\\$][a-zA-Z0-9_\xc0-\xff\\$]
          )*'
        'name': 'variable.interpolated.perl6fe'
      }
      {
        'begin': '(\\{)'
        'beginCaptures':
          '1':
            'name': 'punctuation.section.embedded.begin.perl6fe'
        'end': '(\\})'
        'endCaptures':
          '1':
            'name': 'punctuation.section.embedded.end.perl6fe'
        'patterns': [
          'include': '$self'
        ]
        'name': 'meta.interpolation.perl6fe'
      }
    ]
